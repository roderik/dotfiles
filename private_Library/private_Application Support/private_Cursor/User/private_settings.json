{
	"window.commandCenter": 1,
	"cursor.chat.premiumChatAutoScrollWhenAtBottom": true,
	"cursor.cmdk.useThemedDiffBackground": true,
	"cursor.diffs.useCharacterLevelDiffs": true,
	"workbench.colorTheme": "Catppuccin Macchiato",
	"workbench.iconTheme": "catppuccin-macchiato",
	"editor.inlineSuggest.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.tabSize": 2,
	"git.enableSmartCommit": true,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit",
		"source.organizeImports": "explicit"
	},
	"git.confirmSync": false,
	"git.autofetch": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"javascript.updateImportsOnFileMove.enabled": "never",
	"workbench.tree.renderIndentGuides": "always",
	"workbench.colorCustomizations": {
		"tree.indentGuidesStroke": "#05ef3c9c"
	},
	"errorLens.fontStyleItalic": true,
	"editor.fontWeight": "normal",
	"editor.minimap.enabled": false,
	"redhat.telemetry.enabled": false,
	"workbench.editor.enablePreview": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js, ${capture}.typegen.ts, ${capture}.spec.ts, ${capture}.test.ts",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.cjs": "${capture}.cjs.map, ${capture}.min.cjs, ${capture}.d.cts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts, ${capture}.typegen.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
		"*.mts": "${capture}.typegen.ts",
		"*.cts": "${capture}.typegen.ts"
	},
	"typescript.tsserver.maxTsServerMemory": 16384,
	"typescript.disableAutomaticTypeAcquisition": true,
	"editor.formatOnSave": true,
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.stickyScroll.enabled": true,
	"githubPullRequests.pullBranch": "never",
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.updateImportsOnFileMove.enabled": "never",
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"editor.codeLens": true,
	"trailing-spaces.trimOnSave": true,
	"workbench.tree.enableStickyScroll": true,
	"git.openRepositoryInParentFolders": "never",
	"editor.wordWrap": "on",
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"[graphql]": {
		"editor.formatOnSave": true
	},
	"workbench.editor.customLabels.patterns": {
		"**/app/**/page.tsx": "${dirname(1)}/${dirname} <page>",
		"**/app/**/layout.tsx": "${dirname(1)}/${dirname} <layout>",
		"**/app/**/route.ts": "${dirname(1)}/${dirname} <route>",
		"**/components/**/index.tsx": "${dirname(1)}/${dirname} <component>"
	},
	"editor.fontFamily": "MonoLisa",
	"editor.lineHeight": 0,
	"editor.fontLigatures": "'ss02' on",
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "comment",
				"settings": {
					"fontStyle": "italic"
				}
			}
		]
	},
	"editor.fontSize": 12,
	"solidity.telemetry": true,
	"solidity.compileUsingRemoteVersion": "v0.8.24+commit.e11b9ed9",
	"solidity.formatter": "forge",
	"editor.accessibilitySupport": "off",
	"javascript.experimental.updateImportsOnPaste": true,
	"js/ts.implicitProjectConfig.target": "ESNext",
	"typescript.experimental.updateImportsOnPaste": true,
	"typescript.preferGoToSourceDefinition": true,
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.surveys.enabled": false,
	"workbench.activityBar.orientation": "vertical",
	"typescript.tsserver.experimental.useVsCodeWatcher": false,
	"explorer.incrementalNaming": "smart",
	"cursor.aipreview.enabled": true,
	"cursor.chat.smoothStreaming": true,
	"cursor.general.enableShadowWorkspace": true,
	"cursor.general.gitGraphIndexing": "enabled",
	"sherlock.userId": "5a4b3f31-01f1-4f92-b3b5-87680d50f816",
	"cursor.chat.showSuggestedFiles": true,
	"typescript.tsserver.watchOptions": {
		"watchDirectory": "useFsEvents",
		"fallbackPolling": "dynamicPriorityPolling",
		"watchFile": "useFsEventsOnParentDirectory",
		"synchronousWatchDirectory": true
	}
}
